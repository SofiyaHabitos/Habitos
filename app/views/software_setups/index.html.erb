<section class="page-content">
  <div class="container-fluid">
    <p class="page_path">CRM / Customer Login</p>
    <h2 class="header_title">Customer Login <%= link_to '<i class="fa fa-plus"></i>'.html_safe,new_software_setup_path,:class=>"btn"%></h2>
      <div id="dashboard-container">
        <div class="card table-container mb-2">
          <div class="card-body">
            <table class="table table-striped table-datatable table-hover display" id="software_setup">
              <thead>
                <tr>
                   <th>Client</th>
                   <th>Software Privileges</th>
                   <th>Name</th>
                   <th>Email</th>
                   <!-- <th> Attendance Privileges</th> -->
                </tr>
              </thead>
              <tbody>
              <% @software_setups.each do |software_setup| %>                    
                <tr id="<%=software_setup.id%>">
                    <td><%= software_setup.user.tenant.present? ? software_setup.user.tenant.name : " " unless software_setup.user_id == nil %></td>
                    <td><%= (software_setup.software_privileges.to_s == "true")? "Yes" : "No"%></td>
                    <td><%= software_setup.user.name unless software_setup.user_id == nil%></td>
                    <!--td><!%= software_setup.admin_password%></td-->
                    <td><%= software_setup.user.email unless software_setup.user_id == nil%>
                      <div class="side-pop position-absolute">
                        <ul class="list-unstyled mb-0">
                          <li><a href="/software_setups/<%=software_setup.id%>" title="Show" id="show" > <i class="fas fa-eye"></i></a></li>
                          <li><a href="/software_setups/<%=software_setup.id%>/edit" title="Edit" id="edit"> <i class="fa fa-edit" ></i></a></li>
                          <%if CEO? %>
                            <li><a href="/software_setups/software_delete?id= <%=software_setup.id%>" title="Delete" id="destroy" method="delete" class="confirmation"><i class="far fa-trash-alt"></i></a></li>
                            
                            

                          <%end%>
                        </ul>
                      </div> 
                    </td>
                    <!--td><!%= software_setup.head_password%></td-->
                   <!--  <%#attend=software_setup.attendance.to_s%>
                    <td><%#case attend 
                     when ""%> none
                    <%#when "true"%>Manual 
                    <%#when "false"%>Barcode
                    <%#end%></td> -->
                </tr> 
               <% end %>
              </tbody>
            </table>
          </div>
        </div>
        <div class="clearfix"></div>
      </div>
  </div>
</section>
  <script >
  $('.confirmation').on('click', function () {
        return confirm('Are you sure?');
    });
</script>
