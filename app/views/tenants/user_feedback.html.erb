<section class="page-content">
  <div class="container-fluid">
    <p class="page_path">Report / User FeedBack</p>
    <h2 class="header_title filter_with">User FeedBack</h2>
    <div id="dashboard-container">
      <div class="card table-container mb-2">
        <div class="card-body">
          <table class="table table-striped table-datatable display" cellspacing="0" width="100%" id="user_feedback">
            <thead>
              <tr>
                <th>Client Name</th>
                <th>Not Good</th>
                <th>Average</th>
                <th>Good</th>
                <th>Excellent</th>
              </tr>
            </thead>
            <tbody>
              <% @feedback.each do |k| %>
              <tr>
                <td><%=k[:tenant_name]%></td>
                <td><%=k[:rat2]%></td>
                <td><%=k[:rat3]%></td>
                <td><%=k[:rat4]%></td>
                <td><%=k[:rat5]%></td>
              </tr>
              <% end %>
            </tbody>
          </table>
        </div>
      </div>
      <div class="clearfix"></div>
    </div>
  </div>
</section>

