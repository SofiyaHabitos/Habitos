<section class="page-content">
  <div class="container-fluid">
    <p class="page_path">Master / Individual Credentials</p>
    <h2 class="header_title">Individual Credentials</h2>
    <div id="dashboard-container">
      <div class="card table-container">
        <div class="card-body">
          <table class="table table-striped table-datatable table-hover display" id="enrolment">
            <thead>
              <tr>
                <th>Tenant</th>
                <th>Student Name</th>
                <th>User Name</th>
                <th>Password</th>
                <!--  <th>Created_by</th>
                <th>Updated_by</th>
                <th>From date</th>
                <th>To date</th> -->
              </tr>
            </thead>
            <tbody>
              <% @enrolments.each do |enrolment| %>      
                <tr  id='<%=enrolment.id%>' >
                  <td><%= enrolment.tenant.name %></td>
                  <td><%= enrolment.name %></td>
                  <td><%= enrolment.id_number %></td>
                  <td><%= format_date(enrolment.date_of_birth) %></td>
                  <!--td><!%= create(enrolment) %></td>
                  <td><!%= update(enrolment) %></td>
                  <td><!%= enrolment.from_date.strftime("%d-%m-%Y") %></td>
                  <td><!%= enrolment.to_date.strftime("%d-%m-%Y") %></td-->
                </tr>
              <% end %>
            </tbody>
          </table>
        </div>
      </div>
      <div class="clearfix"></div>
    </div>
  </div>
</section>