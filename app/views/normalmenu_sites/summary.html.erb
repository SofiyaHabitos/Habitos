<section class="page-content">
  <div class="container-fluid">
    <p class="page_path">Others / Order Summary</p>
    <h2 class="header_title filter_with">Order Summary
      <%= form_for(@tenant, url: customer_report_path) do |f| %>
        <div class="filter_table">
          <ul class="list-unstyled d-flex align-items-end mb-0">
            <li>
              <div class="control floating_label">
                <%= f.collection_select :id, @tenants, :id, :name, {prompt: ""}, {class: "click_operation input-text", id: "e1",required: true} %>
                <label class="input-text-placeholder" for="date">Select Customer</label>
              </div>
            </li>
            <li>
              <div class="control floating_label">
                <%= f.text_field :from_date, class: "input-text", id: "txtFrom", required: true %>
                <label class="input-text-placeholder" for="date">From</label>
              </div>
            </li>
            <li>
              <div class="control floating_label">
                <%= f.text_field :to_date, class: "input-text", id: "txtTo", required: true%>
                <label class="input-text-placeholder" for="date">To</label>
              </div>
            </li>
            <li>
              <%= f.submit 'Get Report', class: "btn ", id: "report", "data-disable-with": false %>
            </li>
          </ul>
          <!-- <div class="row">
            <div class="col-md-4">
              <%#= f.collection_select :id, @tenants, :id, :name, {prompt: true}, {class: "click_operation input-text", id: "e1",required: true} %>
            </div>
            <div class="col-md-3">

              <%#= f.text_field :from_date, class: "form-control", id: "txtFrom", required: true, placeholder:"dd/mm/yy" %>
            </div>
            <div class="col-md-3">

              <%#= f.text_field :to_date, class: "form-control", id: "txtTo", required: true, placeholder:"dd/mm/yy"%>
            </div>
            <div class="col-md-2 d-inline-flex align-items-center">
              
            </div>
          </div> -->
        </div>
      <% end %>
    </h2>
      <div id="dashboard-container">
        <div class="card table-container mb-2">
          <div class="card-body">
            <table class="table table-striped table-datatable display" id="tenant_detail">            
              <thead>
                <tr>                
                  <th>Date</th>
                  <th>Order No</th>
                  <th>Customer</th>
                  <th>Type</th>
                  <th>Meal Time</th>
                  <th>Meal Restriction</th>                                            
                  <th>Subscribed</th>
                  <!-- <th>Not Enrolled</th> -->
                  <th>Parent Orders</th>
                  <th>Instant Orders</th>
                  <th>Total Packs</th>
                  <th>Actual consumed from Enrolled</th>
                  <th>Actual consumed from Not Enrolled</th>
                  <th>Time</th>
                  <th>Delivery Status</th>
                </tr>
              </thead>
              <tbody>
                <% if @orders.present? %>
                  <% @orders.each do |order| %>
                    <tr>                    
                      <td><%= order[0] %></td>
                      <td><%= order[1] %></td>
                      <td><%= order[2] %></td>
                      <td><%= order[3] %></td>
                      <td><%= order[4] %></td>
                      <td><%= order[5] %></td>
                      <td><%= order[6] %></td>
                      <!-- <td><%#= order[7] %></td> -->
                      <td><%= order[8] %></td>
                      <td><%= order[9] %></td>
                      <td><%= order[10] %></td>
                      <td><%= order[11] %></td>
                      <td><%= order[12] %></td>
                      <td><%= order[13] %></td>
                      <td><%= order[14] %></td>
                    </tr>
                  <% end %>
                <% end %>
              </tbody>
            </table>
          </div>
          <div class="clearfix"></div>
        </div>
      </div>
  </div>

