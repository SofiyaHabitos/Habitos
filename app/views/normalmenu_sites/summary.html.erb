 <script type="text/javascript">
$(function(){
      $("#txtFrom").datepicker({
        //numberOfMonths: 1,
        onSelect: function (selected) {
            var dt = new Date(selected);
            dt.setDate(dt.getDate() );
            $("#txtTo").datepicker("option", "minDate", dt);
        },dateFormat: 'yy-mm-dd'
       
    });
    $("#txtTo").datepicker({
        numberOfMonths: 1,
        onSelect: function (selected) {
            var dt = new Date(selected);
            dt.setDate(dt.getDate()  );
            $("#txtFrom").datepicker("option", "maxDate", dt);
        },dateFormat: 'yy-mm-dd'
    });
    });
</script>

