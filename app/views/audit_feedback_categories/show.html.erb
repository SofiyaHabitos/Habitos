<section class="page-content">
  <div class="container-fluid">
    <p class="page_path">Master / Audit Feedback Category / Show Audit Feedback Category</p>
    <h2 class="header_title">Show Audit Feedback Category <%= link_to '<i class="fas fa-arrow-left"></i>Back'.html_safe, audit_feedback_categories_path, title:"Back", :class=>"btn btn_back"%></h2>
    <div class="card show_form mb-5">
      <div class="card-body">
        <div class="row">

          <div class="col-md-3">
            <label class="control-label">Category Name</label>
            <h3><%= @audit_feedback_category.category_name %></h3>
          </div>

          <div class="col-md-3">
            <label class="control-label">Category Description</label>
            <h3><%= @audit_feedback_category.category_desc %></h3>
          </div>

          <div class="col-md-3">
            <label class="control-label">Status</label>
            <h3><%= @audit_feedback_category.status %></h3>
          </div>
           
        </div>
      </div>
    </div>
</section>





















