                            <div class="form-group">

                                <%= ff.text_field :fibre,:class=>"form-control boxed",min:1, :required=>"true", :value =>production_item.id.nil? ? nil : (production_item.nutrition.nil? ? nil : production_item.nutrition.fibre) %>
                 
