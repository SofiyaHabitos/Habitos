      display: block;
    }
    
    #notifications h3 {
      background-color: #2e60ad;
      padding: 5px 10px;
      color: #fff;
      font-size: 20px;
      height: unset;
      margin-bottom: 0;
    }
    
    #noti_Container {
      position: relative;
    }
    /* A CIRCLE LIKE BUTTON IN THE TOP MENU. */
    
    #noti_Button {
      color: #414042;
      margin: -2px 30px 0 10px;
      cursor: pointer;
      font-size: 18px;
    }
    /* THE POPULAR RED NOTIFICATIONS COUNTER. */
    
    #noti_Counter {
    display: inline-block;
    position: absolute;
    right: 22px;
    top: -2px;
    border: 2px solid #fff;
    min-width: 14px;
    min-height: 14px;
    font-size: 12px;
    font-weight: 700;
    line-height: 1;
    color: #fff;
    text-align: center;
    white-space: nowrap;
    vertical-align: middle;
    background-color: #ee5223;
    border-radius: 50%;
    z-index: 1;
    }
    /* THE NOTIFICAIONS WINDOW. THIS REMAINS HIDDEN WHEN THE PAGE LOADS. */
    
    #notifications {
      display: none;
      width: 430px;
      position: absolute;
      top: 30px;
      right: 0px;
      background: #FFF;
      border: solid 1px rgba(100, 100, 100, .20);
      -webkit-box-shadow: 0 3px 8px rgba(0, 0, 0, .20);
      z-index: 1001;
    }
    /* AN ARROW LIKE STRUCTURE JUST OVER THE NOTIFICATIONS WINDOW */
    
    #notifications:before {
      content: '';
      display: block;
      width: 0;
      height: 0;
      color: transparent;
      border: 10px solid #CCC;
      border-color: rgba(0, 0, 0, 0) rgba(0, 0, 0, 0) #2e60ad;
      position: absolute;
      right: 12px;
      top: -20px;
    }

/*Notications ends*/

