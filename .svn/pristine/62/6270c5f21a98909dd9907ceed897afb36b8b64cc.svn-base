/*Radio*/

.claims_radio [type="radio"]:checked,
.claims_radio [type="radio"]:not(:checked) {
    position: absolute;
    left: -9999px;
}
.claims_radio [type="radio"]:checked + label,
.claims_radio [type="radio"]:not(:checked) + label
{
    position: relative;
    padding-left: 28px;
    cursor: pointer;
    line-height: 20px;
    display: inline-block;
    color: #666;
}
.claims_radio [type="radio"]:checked + label:before,
.claims_radio [type="radio"]:not(:checked) + label:before {
    content: '';
    position: absolute;
    left: 0;
    top: 0;
    width: 18px;
    height: 18px;
    border: 1px solid #d3d3d3;
    border-radius: 100%;
    background: #fff;
}
.claims_radio [type="radio"]:checked + label:after,
.claims_radio [type="radio"]:not(:checked) + label:after {
    content: '';
    width: 10px;
    height: 10px;
    background: #2e60ad;
    position: absolute;
    top: 4px;
    left: 4px;
    border-radius: 100%;
    -webkit-transition: all 0.2s ease;
    transition: all 0.2s ease;
}

.claims_radio [type="radio"]:checked + label.radio_no:after,
.claims_radio [type="radio"]:not(:checked) + label.radio_no:after {
    content: '';
    width: 10px;
    height: 10px;
    background: #fdc006;
    position: absolute;
    top: 4px;
    left: 4px;
    border-radius: 100%;
    -webkit-transition: all 0.2s ease;
    transition: all 0.2s ease;
}

.claims_radio [type="radio"]:not(:checked) + label:after {
    opacity: 0;
    -webkit-transform: scale(0);
    transform: scale(0);
}
.claims_radio [type="radio"]:checked + label:after {
    opacity: 1;
    -webkit-transform: scale(1);
    transform: scale(1);
}

/*Radio Ends*/



/*Checkbox*/

span.fake-input:focus, input[type="checkbox"]:checked + .fake-input:before:focus{
    outline: 2px solid;
    outline-color: #4581dc;
}

span.fake-input2:focus, input[type="checkbox"]:checked + .fake-input2:before:focus{
