<%= form_for(item_subcategory,validate: true) do |f| %>

  <article class="content item-editor-page wrap-center">
    <div class="title-block">
        <h3 class="title"><span class="sparkline bar" data-type="bar"></span> </h3>
    </div>
    <form name="item">
            <div class="row">
                <div class="form-group">
                <div class="col-xs-4">
                <label class="control-label">
                 <%= f.label :name ,"Name"%>
                 </label><div class="form-group"> 
                 <%= f.text_field :name,required:true,class: "form-control boxed " %>
                 </div>
                 </div>
                 </div>
                 </div>

                 <div class="row">
                <div class="form-group">
                 <div class="col-xs-4">
                <label class="control-label">
                 <%= f.label :description %>
                 </label><div class="form-group"> 
                 <%= f.text_area :description,class: "form-control boxed " %>
                 </div>
                 </div>
             </div>
        </div>
        
        
        <div class="row">
            <div class="col col-md-3"><%= link_to "Back", item_subcategories_url ,:class=>"btn btn-primary"%></div>
            <div class="col col-md-3"><%= f.submit "Submit",:class=>"btn btn-primary"%></div>
        </div>
        
  </form>
  </article>
<% end %>

