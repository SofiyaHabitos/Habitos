<div class="col-md-12 col-sm-8 body-wrap">
  <div class="body-container">
    <div class="body-header">
      <h3>User FeedBack</h3>
       
    </div>
    <div class="body-content">     
      <div id="dashboard-container">
        <div class="table-container">
          <table class="table table-striped table-bordered table-hover display" cellspacing="0" width="100%" id="user_feedback">
            <thead>
                <tr>
                          
                          <th>Tenant Name</th>
                          <!-- <th>Rating 1</th> -->
                          <th>Not Good</th>
                          <th>Average</th>
                          <th>Good</th>
                          <th>Excellent</th>
                </tr>
            </thead>
            <tbody>
            <tr>
            <% @feedback.each do |k| %>
            <td><%=k[:tenant_name]%></td>
            <!-- <td><%=k[:rat1]%></td> -->
            <td><%=k[:rat2]%></td>
            <td><%=k[:rat3]%></td>
            <td><%=k[:rat4]%></td>
            <td><%=k[:rat5]%></td>
                      </tr> 
                   <% end %>
            </tbody>
          </table>
            
        </div>
        <div class="clearfix"></div>
      </div>
    </div>
  </div>
  <div class="clearfix"></div>
</div>
 